{
  "name": "@flink/common",
  "version": "0.0.1",
  "private": true,
  "main": "./dist/index.js",
  "scripts": {
    "build": "yarn db:generate && tsc && cp -r prisma/generated dist/prisma",
    "db:generate": "npx prisma generate --schema ./prisma/schemas/farcaster.prisma",
    "db:push:farcaster": "npx prisma db push --schema ./prisma/schemas/farcaster.prisma"
  },
  "exports": {
    "./queues": "./dist/queues/index.js",
    "./types": "./dist/types/index.js",
    "./entity": "./dist/entity/index.js",
    "./prisma/farcaster": "./dist/prisma/farcaster.js",
    "./mongo": "./dist/mongo/index.js",
    "./utils": "./dist/utils/index.js"
  },
  "dependencies": {
    "@prisma/client": "5.7.1",
    "bullmq": "^5.1.0",
    "metascraper": "^5.43.4",
    "mongodb": "^6.3.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "jest": "^29.7.0",
    "prisma": "5.7.1",
    "prisma-json-types-generator": "^3.0.3",
    "typescript": "^5.3.3"
  }
}
